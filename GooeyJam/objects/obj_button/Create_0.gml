function draw_widget(w_x, w_y, w_width, w_height)
{
	draw_set_color(c_green);
	draw_rectangle(w_width/2 - width/2, w_y + w_height/2 - height/2, w_width/2 + width/2, w_y + w_height/2 + height/2, false);
	draw_set_color(c_white);
}

length += height;